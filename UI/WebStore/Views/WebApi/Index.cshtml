@model IEnumerable<string>
@{ 
    ViewData["Title"] = "Test WebApi";
}

<div class="container">
    <h3>Тест Web API</h3>
    <hr />

    <table class="table">
        <thead>
            <tr>
                <th>#</th>
                <th>Value</th>
            </tr>
        </thead>
        <tbody>
            @{ int i = 1; }
            @foreach (var value in Model)
            {
                <tr>
                    <td>@(i++)</td>
                    <td>@value</td>
                </tr>
            }
        </tbody>
    </table>
</div>